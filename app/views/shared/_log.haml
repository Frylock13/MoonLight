- if current_user
  .frame{style: "max-height: #{current_user.player.frame_size * 100}px;"}
    .log.col-md-8
      - @logs.each do |log|
        - cache log do
          .event
            = raw(log.body)

    .players-online.col-md-4
      %h3 Players online:

      - @online_players.each do |player|
        - cache player do
          .player
            %p 
              = player.name
              = link_to fa_icon("external-link"), player_path(id: player.name)
